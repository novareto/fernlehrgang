<html>
  <body>
    <div metal:use-macro="macro:editform">
    </div>
    <table tal:define="results view/getResults" tal:condition="results">
      <tr>
        <th> Mitgliedsnummer </th>
        <th> Name </th>
      </tr>
      <tr tal:repeat="result results"
          tal:attributes="class python: repeat['result'].odd() and 'odd' or 'even'">
        <td tal:content="result/mnr" />
        <td>
           <a href="#" tal:attributes="href python: view.url(result)">
             <span tal:content="result/name" />
           </a>
         </td>
      </tr>
    </table>  
  </body>
</html>
