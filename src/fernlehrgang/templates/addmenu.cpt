<div class="btn-group context-menu"
     tal:define="menu viewlet.menus.get('addmenu', None)"
     tal:condition="menu and menu.entries">
  <button class="btn btn-primary dropdown-toggle"
          type="button" id="dropdownMenuButton"
          data-toggle="dropdown"
          aria-haspopup="true"
          aria-expanded="false">
    Hinzuf√ºgen
  </button>
  <div class="dropdown-menu">
    <a class="dropdown-item"
       tal:repeat="entry menu.entries"
       href="" tal:attributes="href entry.url();">
      <i class="fas fa-bullseye" tal:attributes="class entry.icon"></i>
      <span tal:content="entry.title"> </span>
    </a>
  </div>
</div>
